{
  "private": true,
  "name": "remix-app-template",
  "description": "",
  "license": "",
  "scripts": {
    "lint": "eslint app/",
    "fix-lint": "npm run lint -- --fix",
    "clean": "rm -rf public/build/ && rm -rf ./build/",
    "size-report": "du -sh public && du -sh build",
    "build": "npm run clean && npm run build:css && remix build && npm run size-report",
    "dev": "remix build && run-p dev:*",
    "dev:node": "cross-env NODE_ENV=development nodemon ./server.js --watch ./server.js",
    "dev:remix": "remix watch",
    "dev:css": "postcss app/styles/global.css --base app/styles --dir app/styles/dist -w",
    "build:css": "postcss app/styles/global.css --base app/styles --dir app/styles/dist --env production",
    "postinstall": "remix setup node",
    "start": "cross-env NODE_ENV=production node ./server.js",
    "db:generate": "prisma generate && eslint --fix app/models/schema/*.ts",
    "db:migrate": "prisma migrate dev"
  },
  "dependencies": {
    "@prisma/client": "^3.13.0",
    "@remix-run/express": "^1.4.3",
    "@remix-run/node": "^1.4.3",
    "@remix-run/react": "^1.1.3",
    "@remix-validated-form/with-zod": "^2.0.1",
    "@supabase/supabase-js": "^1.30.2",
    "@tailwindcss/forms": "^0.4.0",
    "classnames": "^2.3.1",
    "compression": "^1.7.4",
    "express": "^4.18.1",
    "markdown-it": "^13.0.1",
    "morgan": "^1.10.0",
    "nanoid": "^3.2.0",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-icons": "^4.3.1",
    "remix": "^1.4.3",
    "remix-auth": "^3.2.1",
    "remix-auth-supabase": "^4.0.0",
    "remix-validated-form": "^4.4.1",
    "tiny-invariant": "^1.2.0",
    "uuid": "^8.3.2",
    "zod": "^3.14.3"
  },
  "devDependencies": {
    "@remix-run/dev": "^1.1.3",
    "@tailwindcss/typography": "^0.5.2",
    "@types/markdown-it": "^12.2.3",
    "@types/react": "^17",
    "@types/react-dom": "^17",
    "@types/uuid": "^8.3.4",
    "@typescript-eslint/eslint-plugin": "^5.20.0",
    "@typescript-eslint/parser": "^5.20.0",
    "autoprefixer": "^10.4.2",
    "cross-env": "^7.0.3",
    "eslint": "^8.14.0",
    "eslint-plugin-react": "^7.29.4",
    "nodemon": "^2.0.16",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.4.8",
    "postcss-cli": "^9.1.0",
    "postcss-import": "^14.0.2",
    "pre-commit": "^1.2.2",
    "prisma": "^3.13.0",
    "tailwindcss": "^3.0.22",
    "typescript": "^4.5.5",
    "zod-prisma": "^0.5.4"
  },
  "engines": {
    "node": ">=16"
  },
  "sideEffects": false,
  "pre-commit": [
    "lint"
  ]
}
